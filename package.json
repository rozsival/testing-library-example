{
  "name": "testing-library-example",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "format": "prettier --write './**/*.{js,json,md,ts,tsx}'",
    "start": "yarn start:server & yarn start:app",
    "start:app": "react-scripts start",
    "start:server": "json-server --watch --port 3001 data/users.json",
    "test": "jest --config jest.config.js"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^29.1.2",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.2.0",
    "json-server": "^0.17.0",
    "prettier": "^2.7.1",
    "prettier-plugin-package": "^1.3.0",
    "ts-jest": "^29.0.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  }
}
